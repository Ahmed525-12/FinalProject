@using WebApplication1.ViewModel
@model ExpenseVM
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="padding-fixed container">
<form asp-action="Createe" enctype="multipart/form-data">
    <div class="form-group pb-3">
            <label asp-for="Title" class="text-label"></label>
        <input asp-for="Title" class="form-control">
    </div>
        <div class="form-group form-check pb-3">
        
        <input type="checkbox" class="form-check-input" asp-for="Priority">
            <label asp-for="Priority" class="text-label"></label>

    </div>
    <div class="form-group pb-3">
            <label asp-for="AmountMoney" class="text-label"></label>
        <input asp-for="AmountMoney" class="form-control">
    </div>
        <div asp-validation-summary="All" class="text-danger"></div>

    <button type="submit" class="btn btn-success">Submit</button>
        <a type="button" class="btn btn-outline-warning" asp-action="Index">Back To List</a>

</form>
  </div>
